#include <stdio.h>
#include <string.h>


struct employee {
    char name[25];
    int id;
    char department[20];
    float salary;
    char email[50];
};

int main() {
   
    struct employee emp = {
        "John Doe",          // name
        12345,               // id
        "Human Resources",   // department
        55000.50,            // salary
        "john.doe@company.com" // email
    };
